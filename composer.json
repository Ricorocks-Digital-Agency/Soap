{
    "name": "ricorocks-digital-agency/soap",
    "description": "A SOAP client that provides a clean interface for handling requests and responses.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RicorocksDigitalAgency\\Soap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RicorocksDigitalAgency\\Soap\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "orchestra/testbench": "^6.5",
        "spatie/ray": "^1.17",
        "ext-soap": "*",
        "pestphp/pest": "^1.0"
    },
    "prefer-stable": true,
    "authors": [
        {
            "name": "Luke Downing",
            "email": "luke@ricorocks.agency"
        },
        {
            "name": "Sam Rowden",
            "email": "sam@ricorocks.agency"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "RicorocksDigitalAgency\\Soap\\Providers\\SoapServiceProvider"
            ],
            "aliases": [
                "RicorocksDigitalAgency\\Soap\\Facades\\Soap"
            ]
        }
    }
}
